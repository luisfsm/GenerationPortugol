programa
{
	
	funcao inicio()
	{
		inteiro valorEntrada = 0

		escreva("Qual o valo meu querido : ")
		leia(valorEntrada)
		escreva(valorEntrada,", ")
		enquanto(valorEntrada<=100){
			
			valorEntrada = valorEntrada * 3
			escreva(valorEntrada,", ")
		}
	}
}
